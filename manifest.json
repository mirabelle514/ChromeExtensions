{
    "manifest_version": 3,
    "name": "My Start Page",
    "version": "1.0.0",
    "description": "A customizable start page with bookmark management, theme selection, and search functionality. Created by Mirabelle Doiron.",
    "chrome_url_overrides": {
        "newtab": "start.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_title": "My Start Page Settings"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "author": "Mirabelle Doiron",
    "homepage_url": "https://mirabelledoiron.com/",
    "offline_enabled": true,
    "incognito": "spanning"
}